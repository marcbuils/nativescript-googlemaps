{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "declaration": false,
        "noLib": false,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "lib": [
            "es2016",
            "dom"
        ],
        "baseUrl": ".",
        "paths": {
            "*": [
                "../node_modules/tns-core-modules/*",
                "../node_modules/*"
            ]
        }
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "exclude": []
}
